{
  "expo": {
    "name": "Movie Randomizer ",
    "slug": "movie-randomizer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "I build my first React Native App that shows a random movie from an API and the possibility to share your choice with friends and family. Build with React Native and Styled Components.",
    "githubUrl": "https://github.com/DALA746/project-react-native-app"
  }
}
